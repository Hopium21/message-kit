{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "clean": {
      "outputs": []
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "group#start": {
      "dependsOn": ["^build", "group#build"],
      "persistent": true
    },
    "docs#dev": {
      "dependsOn": ["^build", "docs#build"],
      "persistent": true
    },
    "gm#start": {
      "dependsOn": ["^build", "gm#build"],
      "persistent": true
    },
    "subscription#start": {
      "dependsOn": ["^build", "subscription#build"],
      "persistent": true
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
